{
  "name": "debug-namespace",
  "version": "0.0.2",
  "description": "Extends popular node debug utility with auto-namespacing based on file path",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "debug": "DEBUG=${DEBUG:-debugNS*} nodemon index.js",
    "debug:windows": "set DEBUG=debugNS nodemon index.js",
    "debug:powershell": "$env:DEBUG='debugNS' nodemon index.js",
    "test": "xo && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rot26/node-debug-namespace.git"
  },
  "keywords": [
    "debug",
    "namespace",
    "debugNS"
  ],
  "author": "Chris Coleman <chris@rot-26.com> (https://rot-26.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rot26/node-debug-namespace/issues"
  },
  "homepage": "https://github.com/rot26/node-debug-namespace#readme",
  "dependencies": {
    "debug": "4.1.1"
  },
  "devDependencies": {
    "nodemon": "2.0.9"
  }
}
